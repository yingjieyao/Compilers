func main(){
	int a,b,c;
	double d,e,f,g;
	a=100;
	d=1.5;

	if(a>100){
		b=a;
	}elsif(a==100){
		c=a;
	}else{
		a=100;
	}
	
	for c=0;c<100;c++ {
		a++;
	}

	c=0;
	for ;c<100; {
		c++;
	}

	for ;; {
		if(c==100){
			break;
		}
		c++;
	}
	f=Yao(a,b,c,d);
}

func Yao(a,b,c int;x double){
	double sum=0;
	sum=a+b+c+x;
	return sum;
}
